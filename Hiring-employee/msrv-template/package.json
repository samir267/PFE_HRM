{
  "name": "msrv-template",
  "version": "1.0.0",
  "description": "template of microservices based on node/express ts and mongodb/mongoose",
  "main": "server.ts",
  "scripts": {
    "test": "jest",
    "coverage": "nyc npm run test",
    "precommit": "lint-staged",
    "dev": "NODE_ENV=dev nodemon src/server.ts",
    "start": "node dist/index.js",
    "build": "echo 'hello build'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "eslint": "eslint *.ts",
    "prepare": "husky install",
    "cover:report": "nyc report --reporter=lcov --reporter=text",
    "gendoc": "typedoc --out output/documentation src/utils/string.utils.ts",
    "migration": "ts-node src/utils/migration-cli.ts",
    "seed": "ts-node src/db/seeds/run.ts",
    "seed:specific": "ts-node src/db/seeds/run.ts",
    "e2e:test": "docker compose up --build cypress-tests",
    "start:services": "docker-compose up -d zookeeper kafka kafdrop mongodb msrv-app",
    "test:e2e": "docker-compose up cypress"
  },
  "author": "Pithos Global Technology",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@faker-js/faker": "^9.6.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.14.0",
    "@types/opossum": "^8.1.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "express-validator": "^7.0.1",
    "globals": "^15.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mongodb-memory-server": "^9.2.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^2.0.0",
    "@opentelemetry/instrumentation": "^0.200.0",
    "@opentelemetry/instrumentation-fetch": "^0.200.0",
    "@opentelemetry/instrumentation-http": "^0.200.0",
    "@opentelemetry/instrumentation-pg": "^0.52.0",
    "@opentelemetry/resources": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.32.0",
    "@types/joi": "^17.2.3",
    "axios": "^1.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "hashi-vault-js": "^0.4.16",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.3.1",
    "node-vault": "^0.10.2",
    "opossum": "^8.4.0",
    "readdirp": "^4.1.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
